n_boxes <- stringr::str_pad(n_boxes, width = 2,pad = "0", side = "left")
lapply(n_levels,function(level){
paste0(optional_text,"F",freezer,".","R",level,".B",sapply(n_boxes,rep,repetitions))
}) %>% unlist()
}
